{
	"title": "Parameters",
	"type": "object",
	"required": [
		"username",
		"#password",
		"pid",
		"#sapi_token",
		"domain_region",
		"domain_custom"
	],
	"properties": {
		"username": {
			"type": "string",
			"title": "GD Login",
			"default": "",
			"description": "Admin login to GoodData portal."
		},
		"#password": {
			"type": "string",
			"format": "password",
			"title": "GD Password",
			"description": "Password to admin login to GD portal."
		},
		"pid": {
			"type": "string",
			"title": "GoodData Project ID",
			"description": "GD Project ID"
		},
		"#sapi_token": {
			"type": "string",
			"format": "password",
			"title": "Keboola Storage API Token",
			"description": "SAPI Token"
		},
		"domain_region": {
			"type": "string",
			"title": "KBC Project Region",
			"description": "Region of KBC project",
			"enum": [
				"US",
				"EU"
			],
			"default": "US"
		},
		"domain_custom": {
			"type": "string",
			"title": "GD custom domain",
			"description": "If your GD project is whitelabeled, provide the domain. Format: https://domain.com",
			"default": ""
		}
	}
}